{
  "name": "waf-visualization-tool",
  "version": "1.0.0",
  "description": "AWS WAF Rules Visualization Tool",
  "scripts": {
    "install": "cd frontend && npm install && cd ../backend && npm install",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && node index.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
